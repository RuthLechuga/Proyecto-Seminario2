		public void Main()
		{
            string ExtensionVenta = Dts.Variables["User::ExtensionVenta"].Value.ToString();
            string ExtensionCompra = Dts.Variables["User::ExtensionCompra"].Value.ToString();
            string FolderOrigen = Dts.Variables["User::FolderOrigen"].Value.ToString();

            string[] filesVentas = Directory.GetFiles(FolderOrigen, "*" + ExtensionVenta);
            string[] filesCompras = Directory.GetFiles(FolderOrigen, "*" + ExtensionCompra);

            //sql server
            sqlserver(filesVentas,1);
            sqlserver(filesCompras,2);

            //mysql
            mysql(filesVentas, 1);
            mysql(filesCompras, 2);

        }

        public void mysql(string[] files, int type_command)
        {
            try
            {
                //"localhost.seminariodos201602975.root"
                foreach (string fileName in files)
                {
                    MySqlConnection connection = new MySqlConnection();
                    connection.ConnectionString = "server=localhost; user id=root;database=seminariodos201602975; Password=8564872";
                    connection.Open();

                    int counter = 0;
                    string line;
                    StreamReader SourceFile = new StreamReader(fileName);

                    while ((line = SourceFile.ReadLine()) != null)
                    {
                        if (counter > 0)
                        {
                            MySqlCommand command = new MySqlCommand(identify_command(type_command, line), connection);
                            command.ExecuteNonQuery();
                        }
                        counter++;
                    }

                    SourceFile.Close();
                    Dts.TaskResult = (int)ScriptResults.Success;
                }
            }
            catch (Exception e)
            {
                string FolderError = Dts.Variables["User::FolderError"].Value.ToString();

                using (StreamWriter sw = File.CreateText(FolderError + "\\" + "ErrorLog.log"))
                {
                    sw.WriteLine(e.ToString());
                    Dts.TaskResult = (int)ScriptResults.Failure;
                }
            }

        }

        public void sqlserver(string[] files, int type_command)
        {
            try
            {
                foreach (string fileName in files)
                {
                    SqlConnection connection = new SqlConnection();
                    connection = (Dts.Connections["DESKTOP-M4KODE6.seminariodos201602975"].AcquireConnection(Dts.Transaction) as SqlConnection);
                    int counter = 0;
                    string line;
                    StreamReader SourceFile = new StreamReader(fileName);

                    while ((line = SourceFile.ReadLine()) != null)
                    {
                        if (counter > 0)
                        {
                            SqlCommand command = new SqlCommand(identify_command(type_command,line), connection);
                            command.ExecuteNonQuery();
                        }
                        counter++;
                    }

                    SourceFile.Close();
                    Dts.TaskResult = (int)ScriptResults.Success;
                }
            }
            catch (Exception e)
            {
                string FolderError = Dts.Variables["User::FolderError"].Value.ToString();

                using (StreamWriter sw = File.CreateText(FolderError + "\\" + "ErrorLog.log"))
                {
                    sw.WriteLine(e.ToString());
                    Dts.TaskResult = (int)ScriptResults.Failure;
                }
            }
        }

        public String identify_command(int command, String line)
        {
            switch (command)
            {
                case 1: return command_ventas(line);
                case 2: return command_compras(line);
            }

            return "";
        }

        public String command_ventas(String line)
        {
            string Delimitador = Dts.Variables["User::Delimitador"].Value.ToString();
            string TablaDestino = Dts.Variables["User::TablaDestino"].Value.ToString();

            string[] campos = line.Split(Delimitador.ToCharArray()[0]);
            string query = "INSERT INTO " + TablaDestino
                + "(Fecha,CodigoCliente,NombreCliente,TipoCliente,DireccionCliente,NumeroCliente,"
                + "CodProducto,NombreProducto,GrupoProducto,TipoProducto,CodSucursal,NombreSucursal,"
                + "DireccionSucursal,Region,Departamento,Zona,CodVendedor,NombreVendedor,"
                + "Unidades,PrecioUnitario)"
                + " Values ('"
                + campos[0] + "','" + campos[1] + "','" + campos[2] + "','" + campos[3] + "','" + campos[4]
                + "','" + campos[5] + "','" + campos[6] + "','" + campos[7] + "','" + campos[8]
                + "','" + campos[9] + "','" + campos[10] + "','" + campos[11] + "','" + campos[12]
                + "','" + campos[13] + "','" + campos[14] + "','" + campos[15] + "','" + campos[16]
                + "','" + campos[17] + "','" + campos[19] + "','" + campos[20]
                + "');";

            return query;
        }

